from
( WITH RECURSIVE q AS
(
SELECT  h, 1 AS level, ARRAY[cast(code as text)] AS breadcrumb, code AS groupcrumb1, h.name as name1, h.type as type1,
              '' as groupcrumb2, '' as name2, '' as type2, '' as groupcrumb3, '' as name3, '' as type3, '' as groupcrumb4, '' as name4, '' as type4
FROM    account_account h
WHERE  $X{IN, h.id, man_account_id_list}
UNION ALL
SELECT  hi, q.level + 1 AS level, breadcrumb || cast(code as text), q.groupcrumb1 as groupcrumb1,
              q.name1 as name1, q.type1 as type1,
              case when q.level = 1 then hi.code else q.groupcrumb2 end as groupcrumb2,
              case when q.level = 1 then hi.name else q.name2 end as name2,
              case when q.level = 1 then hi.type else q.type2 end as type2,
              case when q.level = 2 then hi.code else q.groupcrumb3 end as groupcrumb3,
              case when q.level = 2 then hi.name else q.name3 end as name3,
              case when q.level = 2 then hi.type else q.type3 end as type3,
              case when q.level = 3 then hi.code else q.groupcrumb4 end as groupcrumb4,
              case when q.level = 3 then hi.name else q.name4 end as name4,
              case when q.level = 3 then hi.type else q.type4 end as type4
FROM    q
JOIN    account_account hi
ON      hi.parent_id = (q.h).id
)
SELECT  REPEAT('  ', level-1) || (q.h).code as ind_code,
(q.h).id,
(q.h).code,
(q.h).name,
level,
breadcrumb::VARCHAR AS path,
groupcrumb1 AS groep1,
name1,
type1,
groupcrumb2 AS groep2,
name2,
type2,
groupcrumb3 AS groep3,
name3,
type3,
groupcrumb4 AS groep4,
name4,
type4,
(q.h).company_id
FROM    q
ORDER BY
breadcrumb
) as t
   inner join account_account a1 on a1.id = t.id
   inner join res_company cc on cc.id = $P{man_company_id}
   inner join res_company on res_company.id = a1.company_id
   left outer join account_fiscalyear afp on afp.id = $P{man_fiscalyear_id}
   left outer join account_period ap_from on ap_from.id = $P{man_period_fisc_id}
   left outer join account_period ap_to on ap_to.id = $P{man_to_period_fisc_id}
order by cons_path, a1.code;]]>
