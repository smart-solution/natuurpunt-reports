                                          					where account_period.id = $P{man_period_fisc_id})
       						    and 
       						    p2.date_stop <= (select account_period.date_stop from account_period
                                          					where account_period.id = $P{man_to_period_fisc_id})
      					  ) IS NULL then 0.00
            else 		  (
							select sum(debit) 
       						from account_move_line
       						inner join account_period p2 on p2.id = account_move_line.period_id
